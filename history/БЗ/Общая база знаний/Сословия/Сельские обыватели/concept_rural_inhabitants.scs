			[Азначэнне паняцця "Сельскія абывацелі"] (* <-lang_by;; *);;

		<-sc_definition;;
		<= nrel_sc_text_translation: ...
		(* -> rrel_example:
	[Сельские обыватели – сословие крестьян, к которым кроем самих крестьян принадлежали однодворцы, военные поселяне, башкиры и казаки.] (* <-lang_ru;; *);; *);
		...
		(* -> rrel_example:
	[Сельскія абывацелі - саслоўе сялян, да якіх кроем саміх сялян належалі аднадворцы, ваенныя сяляне, башкіры і казакі.] (* <-lang_by;; *);;
	*);;
*);;

